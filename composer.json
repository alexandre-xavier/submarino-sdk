{
    "name": "gpupo/submarino-sdk",
    "description": "SDK Não Oficial para integração a partir de aplicações PHP com as APIs do Submarino Marketplace",
    "keywords": ["submarino", "sdk", "api"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ext-curl" : "*",        
        "gpupo/common-sdk": "~1.0"
    },
    "suggest": {
        "monolog/monolog": "Write Log messages"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "monolog/monolog": "dev-master"
    },
    "autoload": {
        "psr-0": { "Gpupo\\": "src" }
    },
    "autoload-dev": {
        "psr-0": {
            "Gpupo\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
